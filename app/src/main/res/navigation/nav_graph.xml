<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashFragment"
    tools:ignore="UnusedNavigation">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.bibaswan.wipro_telstra.ui.SplashFragment"
        android:label="@string/toolbar_"
        tools:layout="@layout/splash_fragment">
        <action
            android:id="@+id/action_splashFragment_to_charactersFragment"
            app:destination="@id/canadaFragment"
            app:exitAnim="@anim/slide_out_left"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
    </fragment>

    <fragment
        android:id="@+id/canadaFragment"
        android:name="com.bibaswan.wipro_telstra.ui.canadaInfo.CanadaInfosFragment"
        android:label="@string/toolbar_"
        tools:layout="@layout/canada_info_fragment">
    </fragment>
</navigation>